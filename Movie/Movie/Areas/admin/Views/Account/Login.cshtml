                                <img src="~/ManageAdmin/img/logo.svg" alt="">
                            </a>

                            <div class="sign__group">
                                <input asp-for="@Model.Email" class="sign__input" placeholder="Email">
                            </div>

                            <div class="sign__group">
                                <input asp-for="@Model.Password" class="sign__input" placeholder="Password">
                            </div>

                            <div class="sign__group sign__group--checkbox">
                                <input asp-for="@Model.RememberMe" type="checkbox" checked="checked">
                                <label for="remember">Remember Me</label>
                            </div>

                            <button class="sign__btn" type="submit">Sign in</button>

                            <span class="sign__delimiter">or</span>

